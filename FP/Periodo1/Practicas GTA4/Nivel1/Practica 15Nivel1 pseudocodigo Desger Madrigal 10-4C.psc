Algoritmo practica15Nivel1
	
	Ingreso1 = 0
	Ingreso2 = 0
	Ingreso3 = 0
	Escribir "Digite en este orden primera inverision; segunda inversion y tercera inversion"
	Leer Ingreso1 Ingreso2 Ingreso3
	total = Ingreso1+Ingreso2+Ingreso3
	P1 = (Ingreso1*100)/total
	P2 = (Ingreso2*100)/total
	P3 = (Ingreso3*100)/total
	Escribir "La primera persona invirtio: " P1 "%" " La segunda  persona invirtio: " P2 "%" " La tercera persona invirtio: " P3 "%"
	
FinAlgoritmo
